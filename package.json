    ],
    "menus": {
      "commandPalette": [
        {
          "command": "memoryManager.initProject"
        },
        {
          "command": "memoryManager.addMemory"
        },
        {
          "command": "memoryManager.viewMemories"
        },
        {
          "command": "memoryManager.saveSelection"
        },
