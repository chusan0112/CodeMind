        },
        {
          "command": "memoryManager.refreshMemory"
        },
        {
          "command": "memoryManager.exportMemories"
        },
        {
          "command": "memoryManager.importMemories"
        },
        {
          "command": "memoryManager.viewStatistics"
        },
        {
          "command": "memoryManager.injectMemories"
        },
        {
          "command": "memoryManager.getContext"
        },
        {
          "command": "memoryManager.cleanExpiredMemories"
        },
