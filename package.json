        },
        {
          "command": "memoryManager.createTeam"
        },
        {
          "command": "memoryManager.exportForTeam"
        },
        {
          "command": "memoryManager.importFromTeam"
        },
        {
          "command": "memoryManager.syncMemories"
        }
      ],
      "editor/context": [
        {
          "command": "memoryManager.saveSelection",
          "when": "editorHasSelection"
        },
        {
          "command": "memoryManager.verifyCode",
          "when": "editorTextFocus"
        }
      ]
    },
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "memoryManager.autoInit": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoInit.description%"
        },
        "memoryManager.autoInject": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoInject.description%"
        },
        "memoryManager.autoExtract": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoExtract.description%"
        },
        "memoryManager.maxContextTokens": {
          "type": "number",
          "default": 2000,
          "description": "%config.maxContextTokens.description%"
        },
        "memoryManager.maxMemories": {
          "type": "number",
          "default": 50,
          "description": "%config.maxMemories.description%"
        },
        "memoryManager.storagePath": {
          "type": "string",
          "default": ".cursor-memory",
          "description": "%config.storagePath.description%"
        },
        "memoryManager.autoValidate": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoValidate.description%"
        },
        "memoryManager.enableCompletion": {
          "type": "boolean",
          "default": true,
          "description": "%config.enableCompletion.description%"
        },
        "memoryManager.importanceLevels": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": ["critical", "high", "medium", "low"],
          "description": "%config.importanceLevels.description%"
        }
      }
    },
    "views": {
      "memoryManager": [
        {
          "id": "memoryManager.memoryView",
          "name": "%view.memoryView.name%",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "memoryManager",
          "title": "%viewsContainers.activitybar.title%",
          "icon": "$(book)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
}

