{
  "name": "codemind",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.1.0",
  "publisher": "your-publisher-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/codemind.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/codemind/issues"
  },
  "homepage": "https://github.com/your-username/codemind#readme",
  "keywords": [
    "cursor",
    "ai",
    "memory",
    "context",
    "code-assistant",
    "intellisense",
    "codemind",
    "ai-context",
    "project-memory"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:memoryManager.initProject",
    "onCommand:memoryManager.addMemory",
    "onCommand:memoryManager.viewMemories",
    "onCommand:memoryManager.saveSelection",
    "onCommand:memoryManager.verifyCode",
    "onCommand:memoryManager.refreshMemory",
    "onCommand:memoryManager.exportMemories",
    "onCommand:memoryManager.importMemories",
    "onCommand:memoryManager.injectMemories",
    "onCommand:memoryManager.getContext",
    "onCommand:memoryManager.cleanExpiredMemories",
    "onCommand:memoryManager.autoAdjustImportance",
    "onCommand:memoryManager.viewStatistics",
    "onCommand:memoryManager.test",
    "onCommand:memoryManager.extractFromConversation",
    "onCommand:memoryManager.checkCursorApi",
    "onCommand:memoryManager.applyTemplate",
    "onCommand:memoryManager.viewTemplates",
    "onCommand:memoryManager.applyBestPractices",
    "onCommand:memoryManager.viewBestPractices",
    "onCommand:memoryManager.recognizePatterns",
    "onCommand:memoryManager.createVersion",
    "onCommand:memoryManager.viewVersions",
    "onCommand:memoryManager.rollbackVersion",
    "onCommand:memoryManager.createTeam",
    "onCommand:memoryManager.exportForTeam",
    "onCommand:memoryManager.importFromTeam",
    "onCommand:memoryManager.syncMemories"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "localizations": [
      {
        "languageId": "zh-cn",
        "translations": [
          {
            "id": "vscode",
            "path": "./package.nls.zh-cn.json"
          }
        ],
        "languageName": "简体中文",
        "localizedLanguageName": "简体中文"
      },
      {
        "languageId": "zh-tw",
        "translations": [
          {
            "id": "vscode",
            "path": "./package.nls.zh-tw.json"
          }
        ],
        "languageName": "繁體中文",
        "localizedLanguageName": "繁體中文"
      },
      {
        "languageId": "ja",
        "translations": [
          {
            "id": "vscode",
            "path": "./package.nls.ja.json"
          }
        ],
        "languageName": "日本語",
